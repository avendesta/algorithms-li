package lab11_undirected;


public class HasCycle extends BreadthFirstSearch {
	
	public HasCycle(Graph graph) {
		super(graph);
	}
	
	//TO-DO
	public boolean hasCycle() {
		int numberOfEdges = graph.edges().size();
		int numberOfVertices = graph.vertices.size();
		return numberOfEdges >= numberOfVertices;
	}
}
